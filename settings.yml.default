---
nzbget:
  downloads: /storage/downloads/nzbget
rutorrent:
  downloads: /storage/downloads/rutorrent
plex:
  tag: public
  transcodes: /home/{{user}}/transcodes
rclone:
  version: latest
  cache_backend: /storage/cache-backend
  upload_tmp_path: /storage/upload-tmp
backup:
  tar_dest: /home/{{user}}/Backups
  rsync_dest: rsync://somehost.com/Backups
  rclone_dest: google:/Backups
  keep_local_copy: true
  use_rsync: false
  use_rclone: false
  cron_time: weekly
  cron_state: absent
